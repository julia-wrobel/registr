Theta_h_i  = splines::bs(index, knots = register_step2a$hinv_innerKnots[[id]], intercept = TRUE)
index_reg  = as.vector(Theta_h_i \%*\% register_step2a$hinv_beta[,id])
warp_dat_i = data.frame(index_observed   = index,
